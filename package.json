{
  "name": "prisub-frontend",
  "version": "0.1.0",
  "description": "Front end for the Print Subscription project",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin newsub.html -d dist --htmlmin -o dist/newsub.html && usemin thankyou.html -d dist --htmlmin -o dist/thankyou.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "copyfiles": "^1.2.0",
    "cssmin": "^0.4.3",
    "del": "^3.0.0",
    "grunt": "^1.0.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-imagemin": "^2.0.1",
    "grunt-contrib-uglify": "^3.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^2.0.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^3.0.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.2.2",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.1",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap-validator": "^0.11.9",
    "font-awesome": "^4.7.0"
  }
}
